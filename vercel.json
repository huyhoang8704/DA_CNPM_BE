{
    "version": 2,
    "builds": [
      {
        "src": "index.js", 
        "use": "@vercel/node", 
        "config": {}
      }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "index.js"
        },
        {
          "src": "/api-docs",
          "dest": "swagger.yaml"
        }
    ]
  }
  